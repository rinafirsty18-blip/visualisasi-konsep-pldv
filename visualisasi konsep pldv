  <script>
        let animationStep = 0;
        let isAnimating = false;

        const defaultConfig = {
            shop_name: "VN COFFEE SHOP",
            rey_order: "1 Donat + 1 Susu",
            beny_order: "2 Donat + 1 Susu",
            background_color: "#FF8C69",
            shop_color: "#2C1810",
            rey_shirt_color: "#808080",
            beny_shirt_color: "#87CEEB",
            menu_background: "#FFFACD"
        };

        async function onConfigChange(config) {
            const shopName = config.shop_name || defaultConfig.shop_name;
            const reyOrder = config.rey_order || defaultConfig.rey_order;
            const benyOrder = config.beny_order || defaultConfig.beny_order;
            const backgroundColor = config.background_color || defaultConfig.background_color;
            const shopColor = config.shop_color || defaultConfig.shop_color;
            const reyShirtColor = config.rey_shirt_color || defaultConfig.rey_shirt_color;
            const benyShirtColor = config.beny_shirt_color || defaultConfig.beny_shirt_color;
            const menuBackground = config.menu_background || defaultConfig.menu_background;

            document.getElementById('shopName').textContent = shopName;
            document.getElementById('menuTitle').textContent = `DAFTAR HARGA - ${shopName}`;
            document.getElementById('reyOrderText').textContent = reyOrder;
            document.getElementById('benyOrderText').textContent = benyOrder;
            
            document.body.style.background = `linear-gradient(135deg, ${backgroundColor} 0%, #FFB347 50%, #FFA500 100%)`;
            document.querySelector('.coffee-shop').style.background = `linear-gradient(145deg, ${shopColor}, #3E2723)`;
            document.querySelector('.rey .character-body').style.background = `linear-gradient(145deg, ${reyShirtColor}, #A9A9A9)`;
            document.querySelector('.beny .character-body').style.background = `linear-gradient(145deg, ${benyShirtColor}, #4682B4)`;
            document.querySelector('.menu-board').style.background = menuBackground;
        }

        function mapToCapabilities(config) {
            return {
                recolorables: [
                    {
                        get: () => config.background_color || defaultConfig.background_color,
                        set: (value) => {
                            config.background_color = value;
                            if (window.elementSdk) window.elementSdk.setConfig({ background_color: value });
                        }
                    },
                    {
                        get: () => config.shop_color || defaultConfig.shop_color,
                        set: (value) => {
                            config.shop_color = value;
                            if (window.elementSdk) window.elementSdk.setConfig({ shop_color: value });
                        }
                    },
                    {
                        get: () => config.rey_shirt_color || defaultConfig.rey_shirt_color,
                        set: (value) => {
                            config.rey_shirt_color = value;
                            if (window.elementSdk) window.elementSdk.setConfig({ rey_shirt_color: value });
                        }
                    },
                    {
                        get: () => config.beny_shirt_color || defaultConfig.beny_shirt_color,
                        set: (value) => {
                            config.beny_shirt_color = value;
                            if (window.elementSdk) window.elementSdk.setConfig({ beny_shirt_color: value });
                        }
                    },
                    {
                        get: () => config.menu_background || defaultConfig.menu_background,
                        set: (value) => {
                            config.menu_background = value;
                            if (window.elementSdk) window.elementSdk.setConfig({ menu_background: value });
                        }
                    }
                ],
                borderables: [],
                fontEditable: undefined,
                fontSizeable: undefined
            };
        }

        function mapToEditPanelValues(config) {
            return new Map([
                ["shop_name", config.shop_name || defaultConfig.shop_name],
                ["rey_order", config.rey_order || defaultConfig.rey_order],
                ["beny_order", config.beny_order || defaultConfig.beny_order]
            ]);
        }

        async function startAnimation() {
            if (isAnimating) return;
            
            isAnimating = true;
            document.getElementById('startBtn').disabled = true;
            
            const rey = document.getElementById('rey');
            const beny = document.getElementById('beny');
            const reySpeech = document.getElementById('reySpeech');
            const benySpeech = document.getElementById('benySpeech');
            const totalSummary = document.getElementById('totalSummary');
            const cafeTable = document.getElementById('cafeTable');
            const windowLight = document.getElementById('windowLight');
            const reyItems = document.getElementById('reyItems');
            const benyItems = document.getElementById('benyItems');

            // Step 1: Characters walk to coffee shop
            rey.classList.add('walking');
            beny.classList.add('walking');
            
            setTimeout(() => {
                rey.style.left = '35%';
                beny.style.left = '45%';
            }, 100);

            // Step 2: Show Rey's speech
            setTimeout(() => {
                rey.classList.remove('walking');
                beny.classList.remove('walking');
                reySpeech.classList.add('show');
            }, 2500);

            // Step 3: Show Beny's speech
            setTimeout(() => {
                reySpeech.classList.remove('show');
                benySpeech.classList.add('show');
            }, 4000);

            // Step 4: Show purchased items
            setTimeout(() => {
                benySpeech.classList.remove('show');
                reyItems.classList.add('show');
                benyItems.classList.add('show');
            }, 5500);

            // Step 5: Show total summary
            setTimeout(() => {
                totalSummary.classList.add('show');
            }, 7000);

            // Step 6: Characters move to table
            setTimeout(() => {
                rey.style.left = '50%';
                beny.style.left = '60%';
                rey.style.bottom = '15%';
                beny.style.bottom = '15%';
                cafeTable.classList.add('show');
            }, 8500);

            // Step 7: Show window light (ending scene)
            setTimeout(() => {
                windowLight.classList.add('show');
                isAnimating = false;
                document.getElementById('startBtn').disabled = false;
            }, 10000);
        }

        function resetAnimation() {
            if (isAnimating) return;
            
            const rey = document.getElementById('rey');
            const beny = document.getElementById('beny');
            const reySpeech = document.getElementById('reySpeech');
            const benySpeech = document.getElementById('benySpeech');
            const totalSummary = document.getElementById('totalSummary');
            const cafeTable = document.getElementById('cafeTable');
            const windowLight = document.getElementById('windowLight');
            const reyItems = document.getElementById('reyItems');
            const benyItems = document.getElementById('benyItems');
            // Reset positions
            rey.style.left = '5%';
            beny.style.left = '15%';
            rey.style.bottom = '25%';
            beny.style.bottom = '25%';
            
            // Hide all elements
            rey.classList.remove('walking');
            beny.classList.remove('walking');
            reySpeech.classList.remove('show');
            benySpeech.classList.remove('show');
            totalSummary.classList.remove('show');
            cafeTable.classList.remove('show');
            windowLight.classList.remove('show');
            reyItems.classList.remove('show');
            benyItems.classList.remove('show');
            
            animationStep = 0;
        }



        // Initialize SDK
        if (window.elementSdk) {
            window.elementSdk.init({
                defaultConfig,
                onConfigChange,
                mapToCapabilities,
                mapToEditPanelValues
            });
        }
    </script>
